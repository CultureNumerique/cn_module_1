              <response_label ident="I_C25948EB">
                <material>
                  <mattext texttype="text/html"><![CDATA[<p>A la fin de la seconde guerre mondiale</p>]]></mattext>
                </material>
              </response_label>
              <response_label ident="I_8678F3E5">
